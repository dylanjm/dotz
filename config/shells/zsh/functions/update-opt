#!/usr/bin/env zsh

function update-opt() {
  local case="${1}"
  local name="${2}"
  local type="${3}"
  local longcase

  if [[ "${case}" == "D" ]]; then
    longcase="Deregulated"
  else
    longcase="Regulated"
  fi
  rsync -e ssh sawtooth1:"/scratch/mcdodyla/heron_cases/2020/12/run/${longcase}/IL_${name}_${type}/${case}_${name}_${type}_o/opt_soln_0.csv" ./
}

# Local Variables:
# mode: shell-script
# sh-basic-offset: 2
# End:
